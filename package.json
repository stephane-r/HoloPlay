    "@react-native-community/viewpager": "^5.0.11",
    "@react-navigation/compat": "^5.3.20",
    "@react-navigation/material-bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.7",
    "@sentry/react-native": "^2.4.3",
    "dotenv": "^10.0.0",
    "hh-mm-ss": "^1.2.0",
    "i18next": "^20.3.1",
    "react": "17.0.2",
    "react-i18next": "^11.10.0",
    "react-native": "0.65.1",
    "react-native-animation-hooks": "^1.0.1",
    "react-native-bootsplash": "^3.2.3",
    "react-native-config": "^1.4.2",
    "react-native-draggable-flatlist": "^2.6.2",
    "react-native-draggable-grid": "^2.1.3",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.7.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-music-control": "^1.4.0",
    "react-native-paper": "^4.9.1",
    "react-native-quick-actions": "^0.3.13",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.6.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.1.1",
    "react-native-svg-icon": "^0.10.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-version": "^4.0.0",
    "react-native-video": "^5.1.1",
    "react-query": "2",
    "react-waterfall": "^4.0.4",
    "rn-dominant-color": "^1.7.2",
    "rn-fetch-blob": "^0.12.0",
    "rn-placeholder": "^3.0.3",
    "semver-compare": "^1.0.0",
    "shelljs": "^0.8.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/hh-mm-ss": "^1.2.1",
    "@types/react": "^17.0.8",
    "@types/react-native": "^0.64.8",
