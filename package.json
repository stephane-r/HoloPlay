{
  "name": "holoplay",
  "version": "1.9.5",
  "private": false,
  "scripts": {
    "start": "react-native start -- --reset-cache",
    "android:run": "./scripts/android-run.sh",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:release:debug": "cd android && ./gradlew assembleDebug",
    "version:major": "node ./scripts/git-push-tag major",
    "version:minor": "node ./scripts/git-push-tag minor",
    "version:patch": "node ./scripts/git-push-tag patch",
    "clean:log": "rm *.log",
    "prettify": "prettier --write src/**/*.js",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint:es": "eslint src",
    "lint:ts": "./node_modules/.bin/tsc",
    "static-analysis": "yarn lint",
    "postversion": "RNV=android react-native-version"
  },
  "resolutions": {
    "**/**/node-fetch": "2.6.1"
  },
  "dependencies": {
