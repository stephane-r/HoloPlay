        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/hh-mm-ss": "^1.2.1",
        "@types/react": "^17.0.8",
        "@types/react-native": "^0.64.8",
        "@types/react-native-snap-carousel": "^3.8.3",
